<h1>Invite Members</h1>

<%= form_with url: invite_tenancy_members_path  do |form|%>
    <%= form.label :email %>
    <%= form.text_field :email, placeholder: 'Email' %>

    <%= form.submit 'Invite' %>
<%end%>

<h1>Members</h1>
<% @member.each do |member|%>
    <%= member.user.email %>
    <%= member.roles %>
  
<%end%>